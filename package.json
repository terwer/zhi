{
  "name": "zhi-dist",
  "version": "1.0.0-alpha.1",
  "description": "dist for zhi to publish to siyuan-note theme market",
  "keywords": [
    "zhi",
    "dist"
  ],
  "author": "terwer",
  "license": "GPL",
  "scripts": {
    "backup": "git archive --format=zip --output=/Users/terwer/Downloads/zhi-dist.zip HEAD",
    "reset": "git pull origin main --force && git rm .",
    "recover": "unzip /Users/terwer/Downloads/zhi-dist.zip -d /Users/terwer/Documents/mydocs/zhi-dist && git add -A",
    "showlog": "git-changelog -c angular --sections feat,fix,refactor,chore,docs -t angular -T",
    "tag": "git push --tags",
    "prepare": "husky install",
    "commitinit": "npx husky add .husky/commit-msg  'npx --no -- commitlint --edit ${1}'",
    "cz": "git-cz",
    "commit": "commit"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-angular": "^17.6.1",
    "@commitlint/cz-commitlint": "^17.5.0",
    "commitizen": "^4.3.0",
    "husky": "^8.0.3",
    "inquirer": "8"
  }
}
