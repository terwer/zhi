{
  "name": "zhi",
  "version": "0.1.0",
  "type": "module",
  "description": "A siyuan-note theme with plugin and blog bundled",
  "homepage": "https://github.com/terwer/zhi",
  "author": "terwer",
  "license": "GPL",
  "keywords": [
    "zhi",
    "siyuan-note",
    "theme",
    "blog"
  ],
  "scripts": {
    "reset": "nx reset",
    "build": "nx run-many --target=build --exclude=zhi-docs",
    "test": "nx run-many --target=test --exclude=zhi-docs",
    "lint": "nx run-many --target=lint --exclude=zhi-docs",
    "serve": "nx serve <project>",
    "cli": "nx cli <project>",
    "clean": "nx clean <project>",
    "update": "nx uodate <project>",
    "publish": "nx publish <project> [--ver=0.0.1] [--tag=latest]",
    "cz": "git-cz",
    "commit": "commit",
    "postinstall": "node ./tools/scripts/patch-nx-cli.cjs"
  },
  "private": true,
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-angular": "^17.6.1",
    "@commitlint/config-nx-scopes": "^17.4.3",
    "@commitlint/cz-commitlint": "^17.5.0",
    "@commitlint/prompt-cli": "^17.6.1",
    "@nrwl/cypress": "15.9.2",
    "@nrwl/eslint-plugin-nx": "15.9.2",
    "@nrwl/js": "15.9.2",
    "@nrwl/linter": "15.9.2",
    "@nrwl/node": "^15.9.2",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/vite": "16.0.1",
    "@nrwl/web": "15.9.2",
    "@nxtensions/astro": "^3.5.0",
    "@types/node": "18.7.1",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "@vitest/coverage-c8": "~0.30.1",
    "@vitest/ui": "^0.30.1",
    "astro": "^2.0.0",
    "commitizen": "^4.3.0",
    "cypress": "^12.11.0",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-prettier": "^4.2.1",
    "glob": "^10.2.2",
    "inquirer": "^8.2.5",
    "jsdom": "~20.0.3",
    "mkdirp": "^3.0.1",
    "nx": "15.9.2",
    "prettier": "^2.6.2",
    "recursive-copy": "^2.0.14",
    "rimraf": "^5.0.0",
    "typescript": "~4.9.5",
    "vite": "^4.3.3",
    "vite-plugin-eslint": "^1.8.1",
    "vite-tsconfig-paths": "^4.0.2",
    "vitest": "^0.30.1"
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "core-js": "^3.6.5",
    "regenerator-runtime": "0.13.7",
    "tslib": "^2.3.0"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  }
}
