{
  "name": "zhi-loader",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/zhi-loader/src",
  "projectType": "library",
  "targets": {
    "dev": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "/Users/terwer/Documents/mydocs/SiYuanWorkspace/public/conf/appearance/themes/zhi-framework",
        "emptyOutDir": false,
        "watch": true
      }
    },
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/zhi",
        "emptyOutDir": false
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["coverage/packages/zhi-loader"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../coverage/packages/zhi-loader"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/zhi-loader/**/*.ts"]
      }
    }
  },
  "tags": []
}
