{
  "name": "zhi-web-blog-vite",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "packages/zhi-web-blog-vite/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nrwl/vite:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/packages/zhi-web-blog-vite"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "serve": {
      "executor": "@nrwl/vite:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "zhi-web-blog-vite:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "zhi-web-blog-vite:build:development",
          "hmr": true
        },
        "production": {
          "buildTarget": "zhi-web-blog-vite:build:production",
          "hmr": false
        }
      }
    },
    "preview": {
      "executor": "@nrwl/vite:preview-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "zhi-web-blog-vite:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "zhi-web-blog-vite:build:development"
        },
        "production": {
          "buildTarget": "zhi-web-blog-vite:build:production"
        }
      }
    },
    "test": {
      "executor": "@nrwl/vite:test",
      "outputs": ["coverage/packages/zhi-web-blog-vite"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../coverage/packages/zhi-web-blog-vite"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/zhi-web-blog-vite/**/*.ts"]
      }
    }
  }
}
